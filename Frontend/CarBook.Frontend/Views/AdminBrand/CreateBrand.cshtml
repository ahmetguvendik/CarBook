@model CarBook.Dto.BrandDTOs.CreateBrandDto

@{
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="card">
    <div class="card-body">
        <h4 class="card-title">Yeni Arac Ozellik Ekleme Sayfasi</h4>
        <p class="card-subtitle mb-4">Sisteme Eklemek Istediginiz Yeni Ozellik Sayfasi </p>

        <form asp-action="CreateBrand"  asp-controller="AdminBrand" method="post">

            <div class="form-group">
                <label>Marka Giriniz</label>
                <input type="text" asp-for="Name" placeholder="Marka Giriniz" class="form-control">
            </div>

            <button type="submit" class="btn btn-success">
                Kaydet
            </button>


        </form>
    </div>

</div>