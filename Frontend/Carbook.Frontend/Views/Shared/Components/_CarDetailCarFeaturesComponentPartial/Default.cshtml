@model List<Carbook.Dto.CarFeaturesDTOs.ResultCarFeaturesDto>

<div class="tab-content" id="pills-tabContent">
						    <div class="tab-pane fade show active" id="pills-description" role="tabpanel" aria-labelledby="pills-description-tab">
						    	<div class="row">
						    		<div class="col-md-4">
									    <ul class="features">
										    @foreach (var item in Model)
										    {
											    @if (@item.IsAvaible)
											    {
												    <li class="check"><span class="ion-ios-checkmark"></span>@item.FeaturesName</li>
											    }
											    else
											    {
												    <li class="remove"><span class="ion-ios-checkmark"></span>@item.FeaturesName</li>
											    }
										    }
									    </ul>
						    		</div>
						    	</div>
						    </div>
						    

						    <div class="tab-pane fade" id="pills-review" role="tabpanel" aria-labelledby="pills-review-tab">
						      <div class="row">
							   		<div class="col-md-7">
							   		</div>
							   		
							   	</div>
						    </div>
						  </div>